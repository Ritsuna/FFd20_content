ast for 1 hour before fading.</li>\n</ul>","chat":"","unidentified":""},"tags":[],"actions":[],"uses":{"per":null,"value":0,"maxFormula":"","max":0,"autoDeductCharges":true,"autoDeductChargesCost":"1"},"attackNotes":[],"effectNotes":[],"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":[],"dictionary":[]},"scriptCalls":[],"featType":"trait","associations":{"classes":[]},"showInQuickbar":false,"abilityType":"su","crOffset":"","disabled":false,"classSkills":{},"activation":{"cost":1,"type":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":"","value":null},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"soundEffect":"","nonlethal":false,"size":"med"},"folder":null,"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}],"system":{"abilities":{"str":{"value":24,"damage":0,"drain":0,"userPenalty":0,"total":24,"mod":7,"carryBonus":0,"carryMultiplier":1,"checkMod":0,"penalty":0,"base":10,"baseMod":0},"dex":{"value":19,"damage":0,"drain":0,"userPenalty":0,"total":19,"mod":4,"checkMod":0,"penalty":0,"base":10,"baseMod":