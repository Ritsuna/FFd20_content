m":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"subType":"feat","associations":{"classes":[]},"showInQuickbar":false,"abilityType":"none","crOffset":"","disabled":false,"classSkills":{},"duration":{"value":null,"units":""},"target":{"value":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":"","value":null},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"soundEffect":"","nonlethal":false,"recharge":{"value":null,"charged":false,"_deprecated":true},"time":{"value":"","_deprecated":true},"damageType":{"value":"","_deprecated":true},"attack":{"parts":[]}},"folder":null,"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"eyxnF3AC62yGJWin","name":"Choco Cure","type":"feat","sort":1800000,"flags":{},"img":"systems/ffd20/icons/ffxiv_icons/FFXIVIcons-MainCommand-(Others)/01_Orders/01/healer_stance.png","effects":[],"system":{"description":{"value":"<p>Bestows healing on itself or an ally within 30 feet for 3d6+4 points of damage. Blue mages may learn this ability as a 2nd level spell (Knowledge: Arcana DC 19).</p>","unidentified":""},"tags":[],"actions":[{"_id":"FzTy6zeucAfuTYWS","name":"Use","img":"systems/ffd20/icons/ffxiv_icons/FFXIVIcons-MainCommand-(Others)/01_Orders/01/healer_stance.png","description":"","tag":"chocoCure","activation":{"cost":1,"type":"standard","unchained":{"cost":1,"type":""}},"duration":{"value":null,"units":"inst"},"target":{"value":"itself or an ally"},"range":{"value":"30","units":"ft","maxIncrements":"1","minValue":null,"minUnits":"personal"},"uses":{"autoDeductChargesCost":"","self":{"value":0,"maxFormula":"","per":null},"per":"day","value":7,"maxFormula":"","max":7},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"heal","attackBonus":"","critConfirmBonus":"","damage":{"parts":[{"formula":"3d6+4","type":{"values":[],"custom":"Healing"}}],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":"","value":null},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":"","damageMult":1,"critRange":20,"critMult":2},"save":{"dc":"0","type":"","description":""},"effectNotes":"","attackNotes":"","soundEffect":"","powerAttack":{"multiplier":"","damageBonus":2,"critMultiplier":1},"naturalAttack":{"primaryAttack":true,"secondary":{"attackBonus":"-5","damageMult":0.5}},"nonlethal":false,"touch":false,"usesAmmo":false,"spellEffect":"","spellArea":"","conditionals":[],"enh":{"value":null}}],"uses":{"per":"day","value":7,"maxFormula":"","max":7,"autoDeductChargesCost":"1"},"attackNotes":[],"effectNotes":[],"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"chocoCure","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"subType":"racial","associations":{"classes":[]},"showInQuickbar":true,"abilityType":"su","crOffset":"","disabled":false,"classSkills":{},"duration":{"value":null,"units":"inst"},"target":{"value":"itself or an ally"},"attackName":"","actionType":"heal","attackBonus":"","critConfirmBonus":"","damage":{"parts":[["3d6+4","Healing"]],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":"","value":null},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":"","damageMult":1,"critRange":20,"critMult":2},"save":{"dc":"0","type":"","description":""},"soundEffect":"","nonlethal":false,"attack":{"parts":[]}},"folder":null,"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"0q91Ng8OZaSyuhXr","name":"Mount - Race - Gold Chocobo","type":"race","sort":2600000,"flags":{"importid":"y0lYDOsyndTGlCwI"},"img":"systems/ffd20/icons/ffxiv_icons/chocobo/Company_Chocobo_Mount_Gold.png","effects":[],"system":{"description":{"value":"<p>The gold-plumed chocobo is a rare sight to behold assuming one can find one.</p>\n<p>STARTING STATISTICS</p>\n<p>Size: Large; Speed: 80 ft., Climb 20 ft., Fly 150 ft. (average), Swim 40 ft.; AC: +9 natural armor; Attack: bite (1d8), claw (1d6); Ability Scores: Str 24, Dex 20, Con 24, Int 10, Wis 18, Cha 12; Special Qualities: darkvision 60&prime;, scent, Low-light vision</p>\n<p>SPECIAL ABILITIES</p>\n<p>Choco Cure (Ex)</p>\n<p>7/day, a gold chocobo can bestow healing on itself or an ally within 30 feet for 3d6+4 points of damage. Blue mages may learn this ability as a 2nd level spell (Knowledge: Arcana DC 19).</p>\n<p>Choco Meteor (Su)</p>\n<p>3/day, a gold chocobo can fire one meteor off towards its target. The gold chocobo summons a molten meteor into the sky to rain your enemies those within the area of effect take 10d6 points of damage (half fire, half earth) and must make a Reflex save (DC 19) for half. Blue mages may learn this ability as a 5th level spell (Knowledge: Arcana DC 25).</p>\n<p>Choco Halo (Su)</p>\n<p>1/day, a gold chocobo casts a halo on its ally. This halo grants Reraise, Float, and Haste status for 1 minute. Blue mage may learn this ability as an 8th level spell (DC 31).</p>\n<p>Choco Trishot (Su)</p>\n<p>1/day, a gold chocobo can blast an opponent with ice, fire and lightning all at once within 30 ft. The gold chocobo must make a ranged touch attack (+17), if it hits, the attack deals 5d6+5 fire damage, 5d6+5 ice damage, and 5d6+5 lightning damage. Blue mages may learn this ability as a 8th level spell (DC 31).</p>\n<p>Acrobatics (Ex)</p>\n<p>Gold chocobos are natural at acrobatics. It gains a +8 bonus on Acrobatics checks. It can also take 10 on Acrobatics checks, even if stress or distractions would otherwise prevent it from doing so.</p>\n<p>Camouflage (Ex)</p>\n<p>Gold chocobos can use Stealth to hide in any sort of natural terrain, even if the terrain does not grant cover or concealment.</p>\n<p>Climbing (Ex)</p>\n<p>Gold chocobos are natural climbers. It has a climb speed of 20 feet and gains a +8 bonus on Climb checks. It can also take 10 on Climb checks, even if stress or distractions would otherwise prevent it from doing so. Also, it is never flat-footed while climbing.</p>\n<p>Stealth (Ex)</p>\n<p>Gold chocobos are natural hiders. It gains a +8 bonus on Stealth checks. It can also take 10 on Stealth checks, even if stress or distractions would otherwise prevent it from doing so. Also, it is never flat-footed while hiding.</p>\n<p>Swimming (Ex)</p>\n<p>Gold chocobos are natural swimmers. It has a swim speed of 40 feet and gains a +8 bonus on Swim checks. It can also take 10 on Swim checks, even if stress or distractions would otherwise prevent it from doing so.</p>\n<p>Water Walking (Ex)</p>\n<p>Gold chocobos have the ability to walk on water while running. A gold chocobo that is running, can walk on water as if it was a solid surface. If it ever stops running, it loses its ability, although it can resume this ability anytime.</p>","unidentified":""},"tags":[],"changes":[{"_id":"di7titwh","formula":"14","operator":"add","subTarget":"str","modifier":"racial","priority":0,"value":0,"target":"ability"},{"_id":"eddmmljh","formula":"10","operator":"add","subTarget":"dex","modifier":"racial","priority":0,"value":0,"target":"ability"},{"_id":"pyouzuvo","formula":"14","operator":"add","subTarget":"con","modifier":"racial","priorit