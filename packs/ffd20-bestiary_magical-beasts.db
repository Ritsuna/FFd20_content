,"weaponGroups":{"value":[],"custom":""},"duration":{"value":null,"units":""},"target":{"value":""},"attackName":"","actionType":"mwak","attackBonus":"","critConfirmBonus":"","damage":{"parts":[["1d8","P"]],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":"","value":null},"bonus":{"formula":""},"label":""},"formula":"","ability":{"attack":"str","damage":"str","damageMult":1,"critRange":20,"critMult":2},"save":{"dc":"0","type":"","description":""},"soundEffect":"","nonlethal":false,"conditionals":[],"attack":{"parts":[]},"size":"med"},"folder":null,"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"ao4cc6DVzTmPhdH5","name":"Endurance","type":"feat","sort":600000,"flags":{},"img":"systems/ffd20/icons/feats/endurance.png","effects":[],"system":{"description":{"value":"<em>Harsh conditions or long exertions do not easily tire you.</em><br/><br/><b>Benefits</b><br/>You gain a +4 bonus on the following checks and saves: Swim checks made to resist nonlethal damage from exhaustion; Constitution checks made to continue running; Constitution checks made to avoid nonlethal damage from a forced march; Constitution checks made to hold your breath; Constitution checks made to avoid nonlethal damage from starvation or thirst; Fortitude saves made to avoid nonlethal damage from hot or cold environments; and Fortitude saves made to resist damage from suffocation.<br/><br/>You may sleep in light or medium armor without becoming fatigued.<br/><br/><b>Normal</b><br/>A character without this feat who sleeps in medium or heavier armor is fatigued the next day.","unidentified":""},"tags":["PFS","General"],"actions":[],"uses":{"per":null,"value":0,"maxFormula":"","max":0,"autoDeductChargesCost":"1"},"attackNotes":[],"effectNotes":[],"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"endurance","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"subType":"feat","associations":{"classes":[]},"showInQuickbar":false,"abilityType":"none","crOffset":"","disabled":false,"classSkills":{},"duration":{"value":null,"units":""},"target":{"value":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":"","value":null},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"soundEffect":"","nonlethal":false,"attack":{"parts":[]}},"folder":null,"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"WzwMRWqQTBWn7Fpr","name":"Run","type":"feat","sort":700000,"flags":{},"img":"systems/ffd20/icons/feats/run.jpg","effects":[],"system":{"description":{"value":"\n                <i>You are swift of foot.</i>\n                <br />\n                <br />\n                \n                <p><b>Benefits</b></p>\n                <p><p class=\"stat-block-1\"> When running, you move five times your normal speed (if wearing medium, light, or no armor and carrying no more than a medium load) or four times your speed (if wearing heavy armor or carrying a heavy load). If you make a jump after a running start (see the Acrobatics skill description), you gain a +4 bonus on your Acrobatics check. While running, you retain your Dexterity bonus to your Armor Class.</p></p>\n                \n            <br />\n                <p><b>Normal</b></p>\n                <p><p class=\"stat-block-1\"> You move four times your speed while running (if wearing medium, light, or no armor and carrying no more than a medium load) or three times your speed (if wearing heavy armor or carrying a heavy load), and you lose your Dexterity bonus to AC.</p></p>\n                \n            \n            ","unidentified":""},"tags":["General","Movement"],"actions":[],"uses":{"per":null,"value":0,"maxFormula":"","max":0,"autoDeductChargesCost":"1"},"attackNotes":[],"effectNotes":[],"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"run","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"subType":"feat","associations":{"classes":[]},"showInQuickbar":false,"abilityType":"none","crOffset":"","disabled":false,"classSkills":{},"duration":{"value":null,"units":""},"target":{"value":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":"","value":null},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"soundEffect":"","nonlethal":false,"recharge":{"value":null,"charged":false,"_deprecated":true},"time":{"value":"","_deprecated":true},"damageType":{"value":"","_deprecated":true},"attack":{"parts":[]}},"folder":null,"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"9mQJ11o68UA2vEWX","name":"Mount - Race - Silver Chocobo","type":"race","sort":800000,"flags":{"importid":"y0lYDOsyndTGlCwI"},"img":"systems/ffd20/icons/ffxiv_icons/chocobo/Company_Chocobo_Mount_Silver.png","effects":[],"system":{"description":{"value":"<p>The silver-plumed chocobo has a tougher plume compared to most other chocobos and shoots lightning out of its beak. The silver chocobo can be found in desert mountains or near dunes.</p>\n<p>STARTING STATISTICS</p>\n<p>Size: Large; Speed: 70 ft., Climb 20 ft., Swim 40 ft.; AC: +5 natural armor; Attack: bite (1d8), claw (1d6); Ability Scores: Str 20, Dex 16, Con 22, Int 8, Wis 16, Cha 12; Special Qualities: darkvision 60&prime;, scent, Low-light vision</p>\n<h5>SPECIAL ABILITIES</h5>\n<h3>Choco Cure (Su)</h3>\n<p>3/day, a silver chocobo can bestow healing on itself or an ally within 30 feet for 3d6+4 points of damage. Blue mages may learn this ability as a 2nd level spell (Knowledge: Arcana DC 19).</p>\n<h3>Choco Guard (Su)</h3>\n<p>3/day, a silver chocobo can create a magical guard around a creature, granting Protect and Shell status for 1d6 rounds. Blue mages may learn this ability as a 2nd level spell (Knowledge: Arcana DC 19).</p>\n<h3>Choco Thunder (Su)</h3>\n<p>5/day, a silver chocobo can shoot lightning at an opponent within 30 ft. The silver chocobo must make a ranged touch attack (+9), if it hits, the attack deals 3d6+5 lightning damage and inflicts the Static status effect for 1d4 rounds (Reflex save DC 13 to negate). Blue mages may learn this ability as a 2nd level spell (Knowledge: Arcana DC 19).</p>\n<h3>Acrobatics (Ex)</h3>\n<p>Silver chocobos are natural at acrobatics. It gains a +8 bonus on Acrobatics checks. It can also take 10 on Acrobatics checks, even if stress or distractions would otherwise prevent it from doing so.</p>\n<h3>Climbing (Ex)</h3>\n<p>Silver chocobos are natural climbers. It has a climb speed of 20 feet and gains a +8 bonus on Climb checks. It can also take 10 on Climb checks, even if stress or distractions would otherwise prevent it from doing so. Also, it is never flat-footed while climbing.</p>\n<h3>Cloudwalking (Ex)</h3>\n<p>Silver chocobos can tread on clouds or fog as though on solid ground.</p>\n<h3>Sandwalking (Ex)</h3>\n<p>Silver chocobos travel at full speed across sand, rocky ground, or dust, and leaves no trail behind unless it chooses to.</p>\n<h3>Surefooted Stride (Ex)</h3>\n<p>Silver chocobos may move through any sort of difficult terrain (such as thorns, overgrown areas, rubble and the like) at its normal speed without taking damage or suffering any other impairment. Areas that have been magically manipulated, however, still effect it.</p>","unidentified":""},"tags":[],"changes":[{"_id":"di7titwh","formula":"10","operator":"add","subTarget":"str","modifier":"racial","priority":0,"value":0,"target":"ability"},{"_id":"eddmmljh","formula":"6","operator":"add","subTarget":"dex","modifier":"racial","priority":0,"value":0,"target":"ability"},{"_id":"pyouzuvo","formula":"12","operator":"add","subTarget":"con","modifier":"racial","priority":0,"value":0,"target":"ability"},{"_id":"th1th0z0","formula":"-2","operator":"add","subTarget":"int","modifier":"racial","priority":0,"value":0,"target":"ability"},{"_id":"9pzkxyod","formula":"6","operator":"add","subTarget":"wis","modifier":"racial","priority":0,"value":0,"target":"ability"},{"_id":"gd04rx0v","formula":"2","operator":"add","subTarget":"cha","modifier":"racial","priority":0,"value":0,"target":"ability"},{"_id":"4ec2vqci","formula":"5","operator":"add","subTarget":"nac","modifier":"racial","priority":0,"value":0,"target":"ac"},{"_id":"kfb5pcgy","formula":"40","operator":"add","subTarget":"landSpeed","modifier":"racial","priority":0,"value":0,"target":"speed"},{"_id":"c8wcu268","formula":"20","operator":"add","subTarget":"climbSpeed","modifier":"racial","priority":0,"value":0,"target":"speed"},{"_id":"7a41dbm0","formula":"40","operator":"add","subTarget":"swimSpeed","modifier":"racial","priority":0,"value":0,"target":"speed"},{"_id":"3k4e1wx7","formula":"11","operator":"add","subTarget":"skill.acr","modifier":"racial","priority":0,"value":0,"target":"skill"},{"_id":"8a11741r","formul