{"_id":"NSfodhalexHDvUkg","name":"Avatar - Race - Diabolos - Starting","type":"race","flags":{"importid":"y0lYDOsyndTGlCwI","cf":{"id":"temp_vmlzon8lkt"}},"img":"systems/ffd20/icons/races/dna.png","system":{"description":{"value":"<p>He is the ruler and creator of the dreamworld known as Dynamis.</p>","unidentified":""},"tags":[],"changes":[{"_id":"di7titwh","formula":"2","operator":"add","target":"ability","subTarget":"str","modifier":"racial","priority":0,"value":0},{"_id":"eddmmljh","formula":"4","operator":"add","target":"ability","subTarget":"dex","modifier":"racial","priority":0,"value":0},{"_id":"pyouzuvo","formula":"3","operator":"add","target":"ability","subTarget":"con","modifier":"racial","priority":0,"value":0},{"_id":"th1th0z0","formula":"0","operator":"add","target":"ability","subTarget":"int","modifier":"racial","priority":0,"value":0},{"_id":"9pzkxyod","formula":"0","operator":"add","target":"ability","subTarget":"wis","modifier":"racial","priority":0,"value":0},{"_id":"gd04rx0v","formula":"6","operator":"add","target":"ability","subTarget":"cha","modifier":"racial","priority":0,"value":0},{"_id":"4ec2vqci","formula":"1","operator":"add","target":"ac","subTarget":"nac","modifier":"racial","priority":0,"value":0},{"_id":"tz8bua4s","formula":"60","operator":"add","target":"speed","subTarget":"flySpeed","modifier":"racial","priority":0,"value":0}],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[]},"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"creatureType":"magicalBeast","size":"med","subTypes":[],"damage":{"parts":[],"critParts":[],"nonCritParts":[]},"uses":{"value":0,"max":0},"conditionals":[]},"ownership":{"default":0,"N1fw6b6C7kQcxuhL":3},"_stats":{"systemId":"ffd20","systemVersion":"10.2.6","coreVersion":"10.303","createdTi