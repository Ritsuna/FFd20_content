{"_id":"A6590rMd4HwnWmkd","name":"Durable Materials","type":"feat","flags":{"importid":"gozsVxPLqbf7gLwa","cf":{"id":"temp_lngouxmya2o"}},"img":"icons/svg/mystery-man.svg","system":{"description":{"value":"<p>At 8th level, the engineer learns how to build for durability and toughness. Any item crafted by the engineer receives a bonus to hit points equal to the level of the engineer at the time of its creation.</p>","unidentified":""},"tags":["Crafting"],"actions":[],"uses":{"per":"","value":0,"maxFormula":"","max":0,"autoDeductChargesCost":"1"},"attackNotes":[],"effectNotes":[],"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"durableMaterials","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"subType":"classFeat","associations":{"classes":["Engineer"]},"showInQuickbar":false,"abilityType":"ex","crOffset":"","disabled":false,"classSkills":{"acr":false,"apr":false,"blf":false,"clm":false,"crf":false,"dip":false,"dev":false,"dis":false,"dri":false,"esc":false,"fly":false,"han":false,"hea":false,"int":false,"kar":false,"kdu":false,"ken":false,"kge":false,"khi":false,"klo":false,"kna":false,"kno":false,"kpl":false,"kre":false,"kte":false,"lin":false,"nav":false,"per":false,"prf":false,"pil":false,"pro":false,"rep":false,"rid":false,"sen":false,"slt":false,"spl":false,"ste":false,"sur":false,"swm":false,"umd":false},"duration":{"value":null,"units":""},"target":{"value":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":"","value":null},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult"