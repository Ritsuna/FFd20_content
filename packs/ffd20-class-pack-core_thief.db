{"_id":"gOLrik3UocXP0WrV","name":"Bag o’ Tricks: Diazepam","type":"feat","img":"icons/consumables/food/berries-ration-round-red.webp","system":{"description":{"value":"<p>Usable a number of times per day equal to half of his thief level (minimum 1), the solid snake can take this item to end shaken and other fear effects.</p>","chat":"","unidentified":""},"tags":[["Equipment"]],"actions":[{"_id":"gB1zvtap127C4mrK","name":"Use","img":"icons/consumables/food/berries-ration-round-red.webp","description":"","tag":"","activation":{"cost":1,"type":"standard"},"unchainedAction":{"activation":{"cost":2,"type":"action"}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"autoDeductCharges":true,"autoDeductChargesCost":"1","self":{"value":0,"maxFormula":"","per":null},"per":"day","value":0,"maxFormula":"max(1,(floor(@classes.thief.lvl/2)))","max":0},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":null,"attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":""},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":"","type":"","description":""},"effectNotes":[],"attackNotes":[],"soundEffect":"","powerAttack":{"multiplier":"","damageBonus":2,"critMultiplier":1},"naturalAttack":{"primaryAttack":true,"secondary":{"attackBonus":"-5","damageMult":0.5}},"nonlethal":false,"usesAmmo":false,"spellEffect":"","spellArea":"","conditionals":[],"enh":{"value":null}}],"uses":{"per":"day","value":0,"maxFormula":"max(1,(floor(@classes.thief.lvl/2)))","autoDeductCharges":true,"autoDeductChargesCost":"1","max":0},"attackNotes":[],"effectNotes":[],"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,